.personalData {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding: 0 0 120rpx;
  position: relative;
}

.personalData .personalData-content {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  background-color: #f5f5f5;
}

.personalData .personalData-content .personalData-content-top {
  width: 100%;
  height: auto;
  background-color: white;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.personalData .personalData-content .personalData-content-top .ul {
  width: 100%;
  height: auto;
}

.personalData .personalData-content .personalData-content-top .ul .li {
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #e9e9e9;
  box-sizing: border-box;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-left {
  width: 150rpx;
  height: 90rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-left span {
  font-size: 30rpx;
  color: #333333;
  line-height: 90rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right {
  width: 560rpx;
  height: 90rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-left {
  margin-right: 45rpx;
  line-height: 90rpx;
  font-size: 26rpx;
  color: #666666;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-left image {
  width: 100%;
  height: 100%;
 
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-left .li-right-left-content {
  width: 100%;
  height: 90rpx;
  overflow: hidden;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-left .li-right-left-content .li-right-left-content-li {
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-left .li-right-left-content .li-right-left-content-li image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-right {
  width: 13rpx;
  height: 22rpx;
  margin-top: 34rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li .li-right .li-right-right image {
  float: left;
  width: 100%;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child {
  height: 150rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child .li-left {
  height: 150rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child .li-left span {
  font-size: 30rpx;
  color: #333333;
  line-height: 150rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child .li-right {
  height: 150rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child .li-right .li-right-left {
  width: 110rpx;
  height: 110rpx;
  margin-top: 20rpx;
  overflow: hidden;
  border-radius: 50%;
  overflow: hidden;
}

.personalData .personalData-content .personalData-content-top .ul .li:first-child .li-right .li-right-right {
  margin-top: 64rpx;
}

.personalData .personalData-content .personalData-content-top .ul .li:last-child {
  border-bottom: none;
}

.personalData .personalData-content .personalData-content-top .personalData-content-top-delect {
  width: 100%;
  padding-top:20rpx;
  height: auto;
  
}

.personalData .personalData-content .personalData-content-top .personalData-content-top-delect image {
  width: 36rpx;
  height: 34rpx;
  float: right;
}

.personalData .personalData-content .personalData-content-bottom {
  width: 100%;
  height: auto;
  background-color: white;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.personalData .personalData-content .personalData-content-bottom .personalData-content-bottom-top text {
  font-size: 30rpx;
  color: #333333;
  line-height: 90rpx;
}

.personalData .personalData-content .personalData-content-bottom .personalData-content-bottom-bottom {
  width: 100%;
  height: auto;

}

.personalData .personalData-content .personalData-content-bottom .personalData-content-bottom-bottom .ul {
  width: 100%;
  height: 300rpx;
  justify-content: space-between;
}

.personalData .personalData-content .personalData-content-bottom .personalData-content-bottom-bottom .ul .li {
  width: 710rpx;
  height: 300rpx;
  overflow: hidden;
  
  margin-bottom: 80rpx;
  float: left;
  
}

.personalData .personalData-content .personalData-content-bottom .personalData-content-bottom-bottom .ul .li image {
  width: 100%;
  height: 140rpx;
}

.line{
height: 20rpx;
width:100%;
background-color: #f5f5f5;
}