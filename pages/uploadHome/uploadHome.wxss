
.spot-select{
  position:fixed;
  width:100%;
  height: 100%;
  top:0px;
  background:#fff;
  z-index: 9999;
  left: 0px;
  overflow: hidden;
}
.uploadSelect{
  height: 300rpx;
  width: 100%;
  margin-top: 400rpx;
}
.upload-type{
  width: 120rpx;
  height: 180rpx;
  float: left;
  margin-left: 97.5rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 50rpx;
  color: #333;
}
.upload-type image{
  width: 120rpx;
  height: 120rpx;
}
.upload-del{
  width: 100%;
  height: 58rpx;
  text-align: center;
}
.quit{
  width: 48rpx;
  height: 48rpx;
  top:20rpx;
  right: 20rpx;
}
.cancel{
  width: 100%;
  text-align: center;
}
.cancel view{
  width: 200rpx;
  height: 60rpx;
  background-color: #1790d7;
  line-height: 60rpx;
  color: #fff;
  border-radius: 6rpx;
  font-size: 30rpx;
  margin: 0 auto;
}
