.head{
  /* display: flex;
  flex-direction: row; */
  position: fixed;
  top: 0;
  left: 20rpx;
  width: 94.666%;
  height: 75rpx;
  margin: 0 auto;
  background-color: #fff;
  z-index: 8;
  
}
.search{
  float: left;
  width: 10%;
  margin-bottom: 3rpx;
}
.search image{
  width:30rpx;
  height:29rpx;
}
.head-select{
  float: left;
}
.head-all{
  float: left;
  font-size: 30rpx;
  line-height: 46rpx;
  color: #333;
}
.head-c{
  line-height: 60rpx;
  width: 30%;
  font-size: 30rpx;
  color: #333333;
  float: left;
  margin-left: 30rpx;
}
.head-c image{
  width: 144rpx;
  height: 31rpx;
}


.head-icon{
   float: left;
  height: 46rpx;
  margin-left: 20rpx;
} 
.head-icon image{
  width: 25rpx;
  height: 14rpx;
  margin-bottom: 10rpx;
  margin-left: 10rpx;
}
.head-recommend{
  float: right;
  width: 200rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  font-size: 24rpx;
  border:1rpx solid #1790D7;
  color: #1790D7;
  border-radius: 30rpx;
}

.head-line{
  width: 100%;
  height: 65rpx;
}
.line{
  width: 100%;
  height: 16rpx;
  background-color: #F5F5F5;
}

.scroll-works{
  display: flex;
  white-space: nowrap; 
}

.content{
  width: 94.666%;
  margin: 0 auto;
  margin-top: 20rpx;
}
.contentInfo{
  width: 100%;
  height: 68rpx;
}
.user_portrait{
  width: 68rpx;
  height: 68rpx;
  float: left;
}
.user_portrait image{
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.user_name{
  height: 68rpx;
  float: left;
  font-size: 24rpx;
  margin-left: 15rpx;
}
.user_name .name{
  height: 34rpx;
  color: #333;
}
.user_name .find{
  height: 34rpx;
  font-size: 24rpx;
  color: #F34755;
}

 .user_name .find image{
  width: 29rpx;
  height: 21rpx;
  margin-left:5rpx;
  top: 3rpx;
  position: relative;
}



.contentInfo-num{
  margin-top: 20rpx;
  float: right;
  font-size: 24rpx;
}
.contentInfo-num-l{
  margin-right: 50rpx;
  float: left;
}
.contentInfo-num-l image{
  width: 28rpx;
  height: 27rpx;
  margin-right:5rpx;
  float: left;
}
.contentInfo-num-l text{
  margin-left:5rpx;
  
  font-size: 24rpx;
  color: #666;
   line-height: 26rpx;
  float: left;
}
.contentInfo-num-r{
  
 
  float:right;
}
.contentInfo-num-r image{
  position: relative;
  width: 30rpx;
  height: 24rpx;
   float: left;
  margin-right:5rpx;
}
.contentInfo-num-r text{
  margin-left:5rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 26rpx;
  float: left;
  
  
}
.addFind{
  height: 46rpx;
  width: 176rpx;
  font-size: 24rpx;
  line-height: 46rpx;
  color: #1790D7;
  text-align: center;
  border: 2rpx solid #1790D7;
  border-radius:6rpx;
  float: left;
  margin-top: 8rpx;
}
.image{
  position: relative;
  margin: 20rpx auto 20rpx auto;
}
.image image{
  width: 100%;
}
.address{
  position: absolute;
  height: 46rpx;
  width: 100%;
  background-color:rgba(0,0,0,0.5);
  left: 0;
  bottom: 10rpx;
  margin: 0 auto;
  text-align: center;
  color: #fff;
  line-height: 46rpx;
  font-size: 26rpx;
}
.address image{
  width: 23rpx;
  height:28rpx;
  top:3rpx;
  position: relative;
}
.address text{
 margin-left:10rpx;
 font-size: 26rpx;
}

