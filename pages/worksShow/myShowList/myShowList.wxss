  border-radius: 6rpx;
  margin-top: 10rpx;
  width: 220rpx;
  height:220rpx;
}
.worksShow-status{
  width: 100%;
  height: 40rpx;
  font-size: 22rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  border-bottom-right-radius:6rpx; 
  border-bottom-left-radius: 6rpx; 
 
  left: 0;
  color: #fff;
  bottom: 10rpx;
  background-color: rgba(0, 0, 0, 0.5)
}

.worksShow-r{
  float: right;
  width:67%;
  height: 200rpx;
}
.worksShow-r .one{
  margin-left: 5rpx;
  height: 60rpx;
  margin-top: 10rpx;
  width:100%;
}
