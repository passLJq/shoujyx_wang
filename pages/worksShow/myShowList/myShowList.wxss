  width: 86%;
  height: 60rpx;
  float: left;
  font-size: 28rpx;
  margin-left: 10rpx;
 
  color: #333;
}
.title-text{
  width: 70%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  float: left;
}
.worksShow-r image{
  width: 50rpx;
  height: 28rpx;
}
.worksShow-r .rm{
  width: 10%;
  height: 36rpx;
  float: right;
}
.worksShow-r .rm image{
  width: 36rpx;
  height: 36rpx;
}
.worksShow-r .number{
  width: 100%;
  height: 60rpx;
  margin-top: 20rpx;
  color: #999;
}
.worksShow-r .number .view{
  width: 45%;
  font-size: 24rpx;
  padding-left: 10rpx;
  float: left;
}
.worksShow-r .number .share{
  width: 50%;
  font-size: 24rpx;
  float: right;
}
.worksShow-r .button{
  width:100%;
  height: 50rpx;
  margin-top: 30rpx;
}
.worksShow-r .button button{
  width:144rpx;
  height: 50rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  text-align: center;
  margin-left: 15rpx;
  float: left;
  padding: 0rpx;
  border-radius: 6rpx;

}
.worksShow-r .button .bg{
  background-color: #1790d7;
  color: #fff;
}
.worksRelease{
