.vote-forward{
    position: fixed;
    width: 88rpx;
    height: 88rpx;
    top: 64%;
    right: 20rpx;
  
    z-index: 1;
}
.vote-share{
    position: fixed;
    width: 88rpx;
    height: 88rpx;
    top: 64%;
    right: 20rpx;
    z-index: 1;
    border-radius: 50%;
    background: #fff;
}
.slide-image{
    width: 100%;
    height: 300rpx;
}
.activity-exp{
    position: relative;
    border: 1rpx solid rgba(153,153,153,0.6);
    width: 700rpx;
    height: 410rpx;
    margin: 46rpx auto 35rpx auto;
}
.activity-exp-title{
    position: absolute;
    font-size: 30rpx;
    color: #ffffff;
    top:-16rpx;
    left:calc(50% - 116rpx);
}
.activity-title-img{
    width: 241rpx;
    height: 43rpx;
}
.activity-title-name{
    position: absolute;
    top: 0rpx;
    left: 58rpx;
    font-size: 30rpx;
    line-height: 43rpx;
}

.activity-title1{
    margin-top: 40rpx;
}
.activity-title2{
     margin-top: 15rpx;
}
